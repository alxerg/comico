{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "src/*.js",
      "options": { "parser": "babylon" }
    },
    {
      "files": "src/components/*.html",
      "options": { "parser": "angular" }
    },
    {
      "files": "src/styles/*.scss",
      "options": { "parser": "scss" }
    }
  ]
}
